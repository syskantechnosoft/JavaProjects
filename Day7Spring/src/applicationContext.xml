<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="product" class="com.uhg.spring.Product">
		<property name="productId" value="150"></property>
		<property name="productName" value="Calculator"></property>
		<property name="price" value="700.0"></property>
	</bean>

	<bean id="user" class="com.uhg.spring.User">
		<property name="userName" value="testUser"></property>
		<property name="password" value="test@123"></property>
	</bean>

</beans>  